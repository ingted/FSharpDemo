@model List<Products.Product>

@{
    ViewBag.Title = "Home Page";
}

<h1>Products</h1>
<div class="row">
    <div class="col-md-4">
        <h3>Product Name</h3>
        @foreach(var product in Model)
        {
            <p>@product.ProductName</p>
        }
    </div>
    <div class="col-md-4">
        <h3>Product Count</h3>
        @foreach (var product in Model)
        {
            <p>@product.ProductCount</p>
        }
    </div>
</div>